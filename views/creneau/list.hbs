<div class="holder">
<div class="title">
    <h3>Creneau List</h3>
</div>


<table class="table table-striped">
    <thead>
        <tr>
            
            <th>heure_debut</th>
            <th>heure_fin</th>
            
            <th></th>
        </tr>
    </thead>
    <tbody>
        {{#each list}}
        <tr>
        
            

            <td>{{this.heure_debut}}</td>
            <td>{{this.heure_fin}}</td>
            
            <td>
                <a href="/creneau/{{this._id}}"><i class="fa fa-pencil fa-lg" aria-hidden="true"></i></a>
                <a href="/creneau/delete/{{this._id}}" onclick="return confirm('Are you sure to delete this record ?');"><i class="fa fa-trash fa-lg" aria-hidden="true"></i></a>
            </td>
        </tr>
        {{/each}}
    </tbody>
</table>